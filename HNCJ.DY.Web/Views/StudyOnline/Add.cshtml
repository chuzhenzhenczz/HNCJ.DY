

@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<h2>Add</h2>
@section header{
    <script type="text/javascript">
        function submitForm() {
            var name = $("#Name").val();
            
            if (name == "") {
                alert("类型名不能为空？");
                return false;
            }
            
            $("form").submit();
        }
    </script>
   
    
    }
@section content{
    <div style="margin-bottom:10px">
			<div>类型名:</div>
			<input class="easyui-textbox"  style="width:100%;height:32px" type="text" id="Name" name="Name"  >
		</div>
   
    }